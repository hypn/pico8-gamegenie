--heading

function draw_heading()
 --gamegenie colours (cycled)
 colors={6, 12, 6, 11, 15, 9, 14, 2}

 --game
 sspr( 0, 0, 8, 8,   2, 4, 16, 16)
 sspr( 8, 0, 8, 8,  16, 4, 16, 16)
 sspr(16, 0, 8, 8,  30, 4, 16, 16)
 sspr(24, 0, 8, 8,  44, 4, 16, 16)
 --genie
 sspr( 0, 0, 8, 8,  62, 4, 16, 16)
 sspr(24, 0, 8, 8,  76, 4, 16, 16)
 sspr(32, 0, 8, 8,  90, 4, 16, 16)
 sspr(40, 0, 8, 8, 100, 4, 16, 16)
 sspr(24, 0, 8, 8, 108, 4, 16, 16)
 
 --cycle colors
 if i>#colors then
  i=1
 end 
 next_color=colors[i]  
 pal(10, next_color)
 
 --slow down color rotation  
 timer+=1
 if timer>2 then
  i+=1  
  timer=0
 end
end


