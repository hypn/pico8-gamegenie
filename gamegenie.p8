--letters

function highlight_letter(col, row)
	hand_col=round((hand_current_x-hand_start_x)/16)
	// 2nd row of letters isn't spaced the same, results in 0.8 so round up
	hand_row=round((hand_current_y-hand_start_y)/20)

	if col==hand_col then
		if row==hand_row then
		 pal(6,7)
		else
		 pal(6,6)
		end	
	else
		pal(6,6)
	end
end

