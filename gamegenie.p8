--hand logic

hand_dest_x=0
hand_dest_y=0

function draw_hand()
	target_x = hand_start_x + (hand_dest_x * 16)
	target_y = hand_start_y + (hand_dest_y * 20)

	if target_y==47 then
		target_y=43
	end
	
	if hand_current_x<target_x then
		hand_current_x+=2
	end	
	if hand_current_x>target_x then
		hand_current_x-=2
	end	
	
	if hand_current_y<target_y then
		hand_current_y+=2
	end	
	if hand_current_y>target_y then
		hand_current_y-=2
	end	
	
 //sspr(64, 0, 16, 8, hand_current_x, hand_current_y, 32, 16)
 sspr(72, 0, 8, 8, hand_current_x, hand_current_y, 20, 16)
end

function update_hand_dest()
	--move left
	if btnp(0) then
		hand_dest_x-=1
		if hand_dest_x<0 then
			hand_dest_x=0
		end
	end

	--move right
	if btnp(1) then
		hand_dest_x+=1
		if hand_dest_x>7 then
			hand_dest_x=7
		end
	end

	--move up
	if btnp(2) then
		hand_dest_y-=1
		if hand_dest_y<0 then
			hand_dest_y=0
		end
	end
	
	--move down
	if btnp(3) then
		hand_dest_y+=1
		if hand_dest_y>4 then
			hand_dest_y=4
		end
	end
end




