function highlight_line(num)
	hand_row=round((hand_current_y-hand_start_y)/20)
	line_num=hand_row-2
	if line_num<0 then
		line_num=0
	end
	
	if num==line_num then
	 pal(6,7)
	else
		pal(6,5)
	end
end

