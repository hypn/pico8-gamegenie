--main

function _init()
 i=1
 timer=0
 spinner=1
 
 // hand drawing and letter highlighting
	hand_start_x=7
	hand_start_y=27
	hand_current_x=hand_start_x
	hand_current_y=hand_start_y
end

function _draw()
 cls()
 draw_heading()
 draw_letters()
 draw_lines()
	draw_hand()
	draw_spinner()
end

function _update()
	update_hand_dest()
end

